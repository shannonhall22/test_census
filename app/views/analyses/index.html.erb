<p id="notice"><%= notice %></p>

<h1>Listing Analyses</h1>

<table>
  <thead>
    <tr>
      <th>Campaign name</th>
      <th>User name</th>
      <th>Agency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analyses.each do |analysis| %>
      <tr>
        <td><%= analysis.campaign_name %></td>
        <td><%= analysis.user_id %></td>
        <td><%= analysis.zip %></td>
        <td><%= link_to 'Show', analysis %></td>
        <td><%= link_to 'Edit', edit_analysis_path(analysis) %></td>
        <td><%= link_to 'Destroy', analysis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analysis', new_analysis_path %>
